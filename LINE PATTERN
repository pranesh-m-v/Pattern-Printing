#include <stdio.h>

int main()
{
    int r, c;
    scanf("%d", &r);
    scanf("%d", &c);
    for(int i=1; i<=r; i++)
    {
        for(int j=1; j<=c; j++)
        {
            if(j == 0 || j == c)
                printf("*");
            else
            {
                for(int k = c; k>2; k--)
                {
                    printf(" ");
                }
            }
        }
        printf("\n");
    }
    
    return 0;
}
